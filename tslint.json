{
  "compilerOptions": {
    "strictNullChecks": true
  },
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [true, "never-prefix"],
    "quotemark": [true, "single"],
    "trailing-comma": false,
    "semicolon": [true, "never"],
    "space-before-function-paren": [true, "always"],
    "no-console": false
  },
  "rulesDirectory": []
}